{
    "name": "chat-gpt-notes",
    "longName": "Chat GPT Notes",
    "description": "Chat GPT Notes",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "format": "prettier --write \"./src/**/*.ts\" \"./src/**/*.tsx\"",
        "start": "concurrently \"pnpm run dev\" \"pnpm run build\"",
        "tailwind": "tailwindcss -i src/styles/index.css -o dist/assets/index.css",
        "dev": "vite",
        "build": "tsc && vite build && pnpm run build:js",
        "build:pre": "esno scripts/prebuild.ts",
        "build:post": "esno scripts/postbuild.ts",
        "build:js": "vite build --config vite.config.content.ts",
        "preview": "vite preview"
    },
    "author": "Doug Silkstone <doug@withseismic.com>",
    "license": "ISC",
    "devDependencies": {
        "@types/chrome": "^0.0.240",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.9.0",
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@types/webextension-polyfill": "^0.10.6",
        "@vitejs/plugin-react": "^4.1.1",
        "autoprefixer": "^10.4.16",
        "concurrently": "^8.2.2",
        "esno": "^0.16.3",
        "fs-extra": "^11.1.1",
        "less": "^4.2.0",
        "postcss": "^8.4.31",
        "prettier": "^3.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwindcss": "^3.3.5",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2",
        "unplugin-auto-import": "^0.16.7",
        "vite": "^4.5.0",
        "vite-plugin-imp": "^2.4.0",
        "vite-plugin-static-copy": "^0.16.0",
        "webext-bridge": "^6.0.1",
        "webextension-polyfill": "^0.10.0",
        "webextension-polyfill-ts": "^0.26.0"
    },
    "dependencies": {
        "@ant-design/cssinjs": "1.18.0-alpha.0",
        "@ant-design/icons": "^5.2.6",
        "@vitejs/plugin-react-refresh": "^1.3.6",
        "antd": "^4.24.14"
    }
}
